{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es2020",
      "module": "commonjs",
      "moduleResolution": "node",
			"rootDir": "./src",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
			"esModuleInterop": true,
			"strict": true,
			"skipLibCheck": true
   }
}
